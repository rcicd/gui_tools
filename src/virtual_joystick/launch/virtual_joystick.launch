<launch>
    <arg name="vehicle"/>

    <node
        pkg="virtual_joystick"
        exec="joystick.py"
        name="joystick_node"
        output="screen"
    >
    <param name="vehicle" value="$(var vehicle)" />
    <param name="assets_dir" value="$(find-pkg-share virtual_joystick)/assets"/>
    </node>
</launch>
